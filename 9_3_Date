import java.util.Date;
public class HQ_9_3_Date {
    public static void main(String [] args)
    {
        long time = 10000;
        Date date = new Date(time);
        for(int count = 0; count < 8; count++)
        {
            System.out.println("The elapsed  milliseconds " +time+ " since Jan 1 1970 is " +
                    date.toString());
            time*=10;
            date.setTime(time);
        }
    }
}
