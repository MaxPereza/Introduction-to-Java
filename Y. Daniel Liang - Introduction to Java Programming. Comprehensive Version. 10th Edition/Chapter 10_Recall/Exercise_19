package pe_10_19;
import java.math.BigInteger;
public class IntegersX {
	public static void main(String [] args) {
		System.out.println("5^(x – 1) – 1              						    x");
		System.out.println("- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - ");
		int x = 2;
		BigInteger a = new BigInteger("5");
		
		for(int i = 1; i < 100; i++) {
			System.out.printf("%-75s %s\n", a.pow(x-1).subtract(BigInteger.ONE) , x);
			x+=1;
		}
	}
}
