package pe_10_06;

public class Display_the_factorial_values {
	public static void main(String []args) {
		
		System.out.println("Facprial " + getFactorial(4) );
	}
	
	static int getFactorial(int number) {
		int factorial= 1;
		for(int i =1 ; i<= number; i++)
			factorial *= i;
		
		return factorial;
	}

}
